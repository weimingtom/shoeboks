---o constructor					/**			* Constructor of the TriggerEvent class			*			* @public			* @return	void			*/			public function TriggerEvent( sTYPE : String ) : void {				super(sTYPE);			}		// -------o public						/**			* set from function			* @public			* @param 			* @return			*/			final public function set from( t : TriggerItem ) : void {				_oTRIGGER = t;			}			/**			* get from function			* @public			* @param 			* @return			*/			public function get from() : TriggerItem {				return _oTRIGGER;			}					// -------o protected		// -------o misc			public static function trc(...args : *) : void {				Logger.log(TriggerEvent, args);			}	}}