* @return	void			*/			public function MapEvent( sTYPE : String , u : uint ) : void {				_uMAPID = u;				super( sTYPE );			}		// -------o public						/**			* Setter of the mapID			* 			* @public			* @param	u : mapID (uint) 			* @return	void			*/			public function set mapID(u : uint ) : void {				_uMAPID = u;			}						/**			* Getter of the mapID			* 			* @public			* @return	mapID	( uint )			*/			public function get mapID() : uint {				return _uMAPID;			}					// -------o protected		// -------o misc			public static function trc(...args : *) : void {				Logger.log(MapEvent, args);			}	}}