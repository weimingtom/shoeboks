	import org.shoebox.core.BoxMath;
	import flash.geom.Rectangle;
	import flash.geom.Point;
	import flash.display.Sprite;
	import org.shoebox.utils.logger.Logger;		/**	* TestProps	* @author shoebox	*/	public class TestProps extends Sprite {				// -------o constructor					/**			* Constructor of the TestProps class			*			* @public			* @return	void			*/
			public function TestProps() : void {
				Logger.level = Logger.LOGLEVEL_LOG;				trc('constructor');				_run();			}		// -------o public				// -------o protected						/**			* 			*			* @param 			* @return			*/			final protected function _run() : void {								trc('run');												var oTRI : Vector.<Point> = Vector.<Point>([														new Point( 200 , 200 ),																								new Point( 300 , 300 ),																								new Point( 100 , 300 )																							]);								var oREC : Rectangle = new Rectangle( 0 , 0 , 240 , 240 );												graphics.lineStyle( 3 , 0xFF0000 );				graphics.moveTo( oTRI[0].x , oTRI[0].y );				graphics.lineTo( oTRI[1].x , oTRI[1].y );				graphics.lineTo( oTRI[2].x , oTRI[2].y );				graphics.lineTo( oTRI[0].x , oTRI[0].y );								graphics.lineStyle( 3 , 0x00FF00 );				graphics.drawRect( oREC.x , oREC.y , oREC.width , oREC.height );												_recTrig( oTRI , oREC );			}						/**			* 			*			* @param 			* @return			*/			final protected function _recTrig( v : Vector.<Point> , r : Rectangle ) : void {							}								// -------o misc			public static function trc(...args : *) : void {				Logger.log(TestProps, args);			}	}}